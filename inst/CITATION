citHeader("Kindly cite the gisfin R package as follows:")

citEntry(entry="misc",
         title = "gisfin R package",
         author = personList(
     person(given ="Juuso", family="Parkkinen", email = "juuso.parkkinen@iki.fi"),
	   person(given ="Leo", family="Lahti"),
	   person(given ="Joona", family="Lehtomaki"),
	   person(given ="Janne", family="Aukia")
         ),
	 journal = "",
	 year = "2014",
         textVersion =
         paste("(C) Juuso Parkkinen, Leo Lahti, Joona Lehtomaki and Janne Aukia 2014.",
               "gisfin R package",
               sep=" ")
         )

citFooter("\nMany thanks for all contributors! For more info, see: https://github.com/rOpenGov/gisfin")
