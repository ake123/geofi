citHeader("Kindly cite the gisfin R package as follows:")

citEntry(entry="misc",
         title = "gisfin R package",
         author = personList(
     person(given ="Joona", family="Lehtomaki", email = "joona.lehtomaki@gmail.com"),
     person(given ="Juuso", family="Parkkinen"),
	   person(given ="Leo", family="Lahti"),
     person(given ="Jussi", family="Jousimo"),
	   person(given ="Janne", family="Aukia")
         ),
	 journal = "",
	 year = "2015",
         textVersion =
         paste("(C) Joona Lehtomaki, Juuso Parkkinen, Leo Lahti, Jussi Jousimo and Janne Aukia 2015-2016.",
               "gisfin R package",
               sep=" ")
         )

citFooter("\nMany thanks for all contributors! For more info, see: https://github.com/rOpenGov/gisfin")
